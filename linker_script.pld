#ifndef __keep_symbol__
OUTPUT_FORMAT("binary")
#endif
ENTRY(start)
phys = 0x00100000;
SECTIONS
{
    .boot 0x00100000 : {
        *(.mboot)
        . = ALIGN(2048);
    }
    .text :
    {
        code = .;
        *(.text)
        . = ALIGN(2048);
    }
    .data :{
        *(.data)
        *(.rodata)
        . = ALIGN(2048);
    }
    .bss :
    {
        bss = .;
        *(.bss)
        . = ALIGN(2048);
    }
    initrd = .;

    end = .;

}